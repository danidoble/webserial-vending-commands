{
  "name": "@danidoble/webserial-vending-commands",
  "description": "WebSerial Vending Commands",
  "version": "1.0.1",
  "type": "module",
  "license": "MIT",
  "author": "danidoble",
  "repository": {
    "type": "git",
    "url": "https://github.com/danidoble/webserial-vending-commands.git"
  },
  "bugs": {
    "url": "https://github.com/danidoble/webserial-vending-commands/issues"
  },
  "homepage": "https://webserial-demo.danidoble.com",
  "keywords": [
    "webserial",
    "vending",
    "machine",
    "api"
  ],
  "files": [
    "dist"
  ],
  "main": "./dist/webserialVendingCommands.cjs",
  "module": "./dist/webserialVendingCommands.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "import": "./dist/webserialVendingCommands.js",
      "require": "./dist/webserialVendingCommands.cjs"
    },
    "./arduino": {
      "import": "./dist/arduino.js",
      "require": "./dist/arduino.cjs"
    },
    "./boardroid": {
      "import": "./dist/boardroid.js",
      "require": "./dist/boardroid.cjs"
    },
    "./jofemar": {
      "import": "./dist/jofemar.js",
      "require": "./dist/jofemar.cjs"
    },
    "./locker": {
      "import": "./dist/locker.js",
      "require": "./dist/locker.cjs"
    },
    "./pinpax": {
      "import": "./dist/pinpax.js",
      "require": "./dist/pinpax.cjs"
    },
    "./relay": {
      "import": "./dist/relay.js",
      "require": "./dist/relay.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint --ext .ts ./lib",
    "format": "prettier --write lib/**/*.ts"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "eslint": "^9.26.0",
    "globals": "^16.1.0",
    "prettier": "3.5.3",
    "typescript": "~5.7.3",
    "typescript-eslint": "^8.32.0",
    "vite": "^6.3.5"
  },
  "publishConfig": {
    "access": "public"
  }
}
